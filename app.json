{
  "expo": {
    "name": "streampel",
    "slug": "streampel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "backgroundColor": "#030014",
    "scheme": "movies",
    "userInterfaceStyle": "dark",
    "splash": {
        "backgroundColor": "#030014",
        "image": "./assets/images/logo.png"
      },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#030014",
        "foregroundImage": "./assets/images/logo.png"
        // "backgroundImage": "./assets/images/logo.png",
        // "monochromeImage": "./assets/images/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "splash": {
        "backgroundColor": "#030014",
        "image": "./assets/images/logo.png",
        "resizeMode": "cover"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          // "imageWidth": 400,
          // "resizeMode": "cover",
          "backgroundColor": "#030014",
          "dark": {
            "backgroundColor": "#030014",
            "image": "./assets/images/logo.png",
            "imageWidth": 400,
            "resizeMode": "cover"
          },
          "android": {
            "backgroundColor": "#030014",
            "image": "./assets/images/logo.png"
            // "imageWidth": 400,
            // "resizeMode": "cover"
          }
        }
      ],
      [
      "@sentry/react-native/expo",
      {
        "url": "https://sentry.io/",
        "project": "streampel",
        "organization": "lindtana-vd"
      }
    ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
